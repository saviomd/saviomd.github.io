	ul.list-unstyled.row.mb-0.js-blog
	.text-right
		a(href='http://saviomd.com/blog/', rel='noopener', target='_blank', ga-on='click', ga-event-category=site.name, ga-event-action='Blog', ga-event-label='Veja mais')= 'Veja mais'
	h3.title.title--sub= 'GitHub Starred'
	ul.post-wrapper--title-one-line.list-unstyled.row.mb-0.js-github-starred
